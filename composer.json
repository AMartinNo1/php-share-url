{
  "autoload": {
    "psr-4": {
      "Nostadt\\ShareUrl\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nostadt\\ShareUrl\\Tests\\": "tests"
    }
  },
  "homepage": "https://github.com/nostadt/php-share-url/",
  "keywords": [
    "social networks",
    "share links",
    "cookieless"
  ],
  "license": "MIT",
  "name": "nostadt/share-url",
  "require": {
